{
  "version": 2,
  "scope": "vibrantxs-projects",
  "builds": [
    {
      "src": "/dist/apps/vibrantx/*",
      "use": "@now/static"
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/dist/apps/vibrantx/src/assets/$1" },
    { "src": "/assets/home(.*)", "dest": "/dist/apps/vibrantx/src/assets/home/$1" },
    { "src": "/(.*).(js|css|ico)", "dest": "/dist/apps/vibrantx/$1.$2" },
    { "src": "/(.*)", "dest": "/dist/apps/vibrantx/index.html" }
  ]
}
